package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int[] numbers1 = { 45, 81, 9, 72, 4, 6};
		 int temp;
		 
		 for(int a=1;;a++)
		 {
			 for(int b=1;b<=numbers1.length-1;b++)
			 {
					if(numbers1[b]>=numbers1[b+1])
					{
						temp=numbers1[b];
						numbers1[b]=numbers1[b+1];
						numbers1[b+1]=temp;
					}
					

			 }
				if(numbers1[0]<=numbers1[1] && numbers1[1]<=numbers1[2] && numbers1[2]<=numbers1[3] && numbers1[3]<=numbers1[4] && numbers1[4]<=numbers1[5] && numbers1[5]<=numbers1[6])
				{
					break;
				}
		 }
		 
			System.out.print("<strong>"+numbers1[0]+" "+numbers1[1]+" "+numbers1[2]+" "+numbers1[3]+" "+numbers1[4]+" "+numbers1[5]+" "+numbers1[6]+"</strong>");   
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 int[] numbers2 = { 6, 5, 2, 8, 1, 9, 13, 4, 11, 7}; 
	}

}
